/*html5*/
article,aside,dialog,footer,header,section,footer,nav,figure,menu{
	display:block;
}
html{
	font-family: "方正兰亭细黑","微软雅黑","Helvetica Neue";
	font-size: 12px;
	background: #ffffff;
}
body{
	overflow-x:hidden ;
}
body,dl,dd,dt{
	margin: 0;
	padding:0;
}
#Doctor{
	height:100vh;
	overflow-x: hidden;
}
ul,ol,li,p ,h1,h2,h3,a{
	margin: 0;
	padding:0;
	list-style: none;
}
a{
	cursor: pointer;
	text-decoration: none;
}
a:active,
a:hover{
	outline: none;
}
img{
	border: none;
}
input,button,textarea,select{
	margin: 0;
	outline: none;
}
textarea{
	overflow: auto;
}
input{
	text-decoration: none;
	border: none;
}
input[type=checkbox]{
	visibility: hidden;
}
h1,h2,h3,h4,h5,h6{
	padding:0;
	margin: 0;
	font-weight: 400;
}
a{
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center;
	color: #000000;
	cursor: pointer;
}

button{
	cursor: pointer;
	list-style: none;
	border: none;
	background: none;
}
img,.img{
	border-color:transparent;
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
}

.btn{
	justify-content: center;
	padding-bottom: 50px;
}
/*顶部菜单*/
#top_nav {
	margin: 0 auto;
	z-index: 1000;
	width:100%;
	height:70px;
}
#top_nav nav{
	display: table;
	height:70px;
	margin: 0 auto;
	position: relative;
	width:1200px;
}
#top_nav nav .logo{
	position: relative;
	float: left;
	margin: 15px 0;
	cursor: pointer;
	border-radius: 3px;
	background-image: url("../image/logo2.png");
	background-repeat: no-repeat;
	background-position: left;
	background-size: 80%;
	background-color: #3f8654;
	width:150px;
	height:40px;
}
#top_nav nav>.logo>i{
	position: absolute;
	right: 0;
	top:0;
	padding: 5px;
	font-size: 20px;
	align-self: center;
	color: #ffffff;
	transition: all 0.3s cubic-bezier(0.68, -0.55, 0.27, 1.55);
	transform:rotate(0deg);
	-ms-transform:rotate(0deg); 	/* IE 9 */
	-moz-transform:rotate(0deg); 	/* Firefox */
	-webkit-transform:rotate(0deg); /* Safari 和 Chrome */
	-o-transform:rotate(0deg);
}
#top_nav .menu_user{
	float: right;
}
#top_nav .menu_user .img{
	float: left;
	width:50px;
	height:50px;
	border: 2px solid #808080;
	margin: 8px;
	position:relative;
	z-index:2;

	/*防止浏览器版本过低*/
	-moz-border-radius: 25px;
	-webkit-border-radius: 25px;
	border-radius: 25px;
	/*解决IE*/

	behavior: url(ie-css3.htc);
}
.menu{
	z-index: 1000;
	display: none;
	position: absolute;
	top:56px;
	left: 0;
	box-shadow: 0px 4px 16px rgba(0, 0, 0, 0.25);
	background: #ffffff;
}
.menu>ul{
	float: right;
	width:220px;
}
.menu>ul>li,.menu>.search{
	cursor: pointer;
	padding: 0 10px;
	height:40px;
	border-bottom: 1px solid #ebebeb;
}
.menu>ul>.active{
	border-left: 2px solid #3f8654;
	color: #3f8654;
}
.menu_sub_content{
	width:440px;
	position: absolute;
	top:0;
	left: 220px;
}
.menu_sub_content{
	background: #ffffff;
	box-shadow: 0px 4px 16px rgba(0, 0, 0, 0.25);
}
.menu_sub_foot{
	width:100%;
	margin: 20px;
	background: #ebebeb;
}
.menu_sub_foot a{
	height:40px;
	color: #3f8654;
}
.menu_sub_content dl{
	box-sizing: border-box;
	padding: 10px 30px;
	width:219px;
}
.menu_sub_content>dl>a{
	padding-bottom: 20px;
	font-size: 12px;
	color: #3f8654;
	height:30px;
	align-items: center;
}
.menu_sub_content dl dt{
	padding: 10px 0;
}
.menu_sub_content dl dt a{
	font-size: 14px;
	color: #3f8654;
}
.menu_sub_content dl dt a i{}
.menu_sub_content dd{
	width:80%;
}
.menu_sub_content>dl:nth-of-type(2n-1){
	border-right: 1px solid #ebebeb;
}
.menu_sub_content>dl:nth-of-type(1),.menu_sub_content>dl:nth-of-type(2){
	border-bottom: 1px solid #ebebeb;
}
.menu_sub_content dd a{
	height:24px;
	font-size: 12px;
}
.menu_sub_content dd a:hover{
	color: #3f8654;
}
.menu_user{
	position: relative;

}


.menu_user>i{
	font-size: 26px;
}
.menu_user .icon-user-bar-message,
.menu_user .icon-caidan{
	display: block;
	text-align: center;
	line-height:70px;
	cursor: pointer;
	color: #7a7a7a;
	position: relative;
}
#top_nav .icon-user-bar-message:before{
	before:"\e604";
}
#top_nav .icon-user-bar-message:after{
	content: "";
	position: absolute;
	top: 24px;
	left: 28px;
	width: 6px;
	height: 6px;
	border-radius: 50%;
	background: #d7000f;
}
.menu_user .user_m{
	float: left;
	height:70px;
	width:44px;
	position: relative;
	z-index: 100;
	color: #666666;
}
.menu_user .user_m div{
	position: absolute;
	left: -40px;
	top: 70px;
}
.menu_user .user_m div:before{
	position: absolute;
	content: "";
	z-index: 1;
	top: -16px;
	left: 55px;
	border: 8px solid transparent;
	border-bottom: 8px solid #cccccc;
}
.menu_user .user_m div:after{
	position: absolute;
	content: "";
	z-index: 1;
	top: -15px;
	left: 55px;
	border: 8px solid transparent;
	border-bottom: 8px solid #ffffff;
}
.menu_user .user_m ul{
	width:90px;
	border:1px solid #cccccc;
	background: #ffffff;
}
.menu_user .user_m ul li {
	cursor: pointer;
	height:30px;
}
.menu_user .user_m ul li a{
	display: block;
	width:90px;
	height:30px;
	text-align: center;
	line-height:30px;
}
.menu_user .user_m ul li:hover{
	background: #f5f5f5;
}
.menu_user .user_m ul li:hover a{
	color: #3f8654;
}
.user_setting{
	float: left;
	position: relative;
	width:40px;
	height:70px;
}
.user_setting div{
	position: absolute;
	top: 70px;
	left: -55px;
}
.user_setting div:before{
	position: absolute;
	content: "";
	z-index: 1;
	top: -18px;
	left: 66px;
	border: 10px solid transparent;
	border-bottom: 8px solid #cccccc;
}
.user_setting div:after{
	position: absolute;
	content: "";
	z-index: 1;
	top: -17px;
	left: 66px;
	border: 10px solid transparent;
	border-bottom: 8px solid #ffffff;
}
.user_setting ul {
	background: #ffffff;
	border: 1px solid #cccccc;
}
.user_setting ul li,
.user_setting ul li a{
	height:30px;
	width:100px;
}
.user_setting ul li a{
	display: inline-block;
	line-height:30px;
	text-align: center;
}
.user_setting ul li:hover{
	background: #f5f5f5;}
.user_setting ul li:hover a{
	color: #3f8654;
}
.user_setting ul li a {
	color: #666666;
}


/***底部组件***/
#foot_bar{
	position: relative;
	display: flex;
	justify-content: space-between;
	flex-direction: column;
	align-items: center;
	width:100%;
	background: #3f8654;
	color: #afe2be;
}
#foot_bar>.line{
	left: 0;
	position: absolute;
	top:0;
	display: flex;
	width:100%;
	height:4px;
	background: #062033;
	border-bottom: 1px solid #245645;
}
#foot_bar>div{
	margin: 0 auto;
	position: relative;
	padding: 25px;
	width:1140px;
}
#foot_bar .company_info{
	float: left;
	width:240px;
}
#foot_bar .company_info img{
	webkit-transform: scale(1, 1);
	transform: scale(1, 1);
}
#foot_bar .company_info img:first-child{
	margin-bottom: 10px;
}
#foot_bar .code{
	float: right;
}
#foot_bar .contact_us{
	position: absolute;
	left: 35%;
}
#foot_bar .contact_us h3 a{
	font-size: 14px;
	color: #ffffff;
	padding-right: 20px;
}
#foot_bar .contact_us h3 a:last-child{
	padding-left:20px ;
	border-left: 1px solid #ffffff ;
}
#foot_bar .contact_us>div img{
	float: left;
}
#foot_bar .contact_us>div>div{
	margin-top: 10px;
	width:340px;
	margin-left:40px ;
}
#foot_bar .contact_us strong{
	font-size: 22px;
	font-weight: 700;
	color: #ffffff;
}
#foot_bar .contact_us small{
	font-size: 16px;
	color: #ffffff;
}
#foot_bar p{
	vertical-align:middle;
	color: #ffffff;
	display: block;
	text-align: center;
	margin: 0 auto;
	padding-bottom: 20px;
}
#foot_bar>p{

	clear: both;
	width: 820px;
	line-height:26px;
}
#foot_bar .code>div:first-child{
	float: left;
	width:115px;
	margin-right: 10px;
}
#foot_bar .code>div:last-child{
	float: right;
	width:115px;
}
#foot_bar>p>a{
	display: inline;
	color: #afe2be;
}
#foot_bar>p>a:hover{
	text-decoration: underline;
	color: #ffffff;
}
/***侧边栏***/
#sidebar{
	z-index: 1000;
	height:100%;
	position: fixed;
	top:0;
	right: 0;
}
#sidebar li{
	width:34px;
	cursor: pointer;
}

#sidebar section{
	width:34px;
	position: absolute;
	top:50%;
	margin-top: -170px;
	right: 0;
}
#sidebar li{
	line-height:34px;
	width:34px;
	height:34px;
	float: right;
	position: relative;
	border-radius: 3px;
	margin: 1px 0;
	background: #9a9a9a;
}
#sidebar li a{
	display: block;
	height:34px;
	width:75px;
}
#sidebar li span{
	color: #ffffff;
	padding-left: 15px;
	height:34px;
	width:100px;
	opacity: 0;
}
#sidebar li:hover{
	height:34px;
	width: 100px;
	background: #0fa732;
}
#sidebar li:hover span{
	opacity: 1;
}
#sidebar i{
	background: url("../image/icon_sidebar.png") no-repeat;
	position: absolute;
	right: 0;
	top: 0;
	width:35px;
	height:35px;
}
#sidebar em{
	position: absolute;
	right: 4px;
	top: 4px;
	width:6px;
	height:6px;
	border-radius: 50%;
	background: #d7000f;
}
#sidebar footer{
	width:34px;
	position: absolute;
	right: 0;
	bottom: 0;
}

/***内容导航横条***/
.section_nav_bar{
	display: block;
	height:44px;
	background: #707070;
	width: 100%;
}
.section_nav_bar>div{
	margin: 0 auto;
	width: 1200px;
}
.section_nav_bar>div>div{
	float: left;
}
.section_nav_bar>div>div span{
	color: #ffffff;
	margin: 0 3px;
	line-height:44px;
}
.section_nav_bar>div>div span a{
	color: #ffffff;
}
/*iconfont*/
.icon-weidian{
	font-size: 20px;
	color: #ffffff;
}
.icon-erweima{
	color: #4d4a4f;
	margin: 10px;
}

.dn{
	display: none !important ;
}
.fixed{
	z-index: 9999;
	top:0;
	position: fixed;
}
.gray{
	-webkit-filter:grayscale(1);
}
#box{
	z-index: 1000;
	width:100%;
	height:100%;
	position: fixed;
	top:0;
	left: 0;
}
.clearFloat{
	clear: both;
}