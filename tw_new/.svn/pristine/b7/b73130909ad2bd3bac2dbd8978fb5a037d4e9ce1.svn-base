/* ��ҳ */
html, body { overflow-x:inherit;  }
/* ͷ�� */
.head{ padding-top:0.2rem; padding-left:0.33rem; padding-right:0.33rem; background:-moz-linear-gradient(left top, #ff6633, #ff9235); background: -webkit-linear-gradient(left top, #ff6633, #ff9235); background: -o-linear-gradient(left top, #ff6633, #ff9235); }
/* ��ַ */
.addrContainer>a{ display:inline-block; }
.address>img{ width:0.34rem; margin:0 0.16rem; }
.address>span{ font-size:0.42rem; color:white; }
.address>img,.address>span{ vertical-align:middle; }
.weather{ float:right; }
.weather>em{ display:inline-block; font-style:normal; }
.weather>em>p{ text-align:right; font-size:0.34rem; }
.weather>em>span{ font-size:0.26rem; }
.weather>em>p,.weather>em>span{ color:white; }
.weather>img{ width:0.74rem; }
.weather>em,.weather>img{ vertical-align:middle; }
/* ���� */
.searchContainer{ margin-top:0.26rem; }
.searchBox{ text-align:center; background:white; border-radius:0.05rem; }
.searchBox>a{ height:0.88rem; line-height:0.88rem; text-align:center; }
.searchBox>a>img{ width:0.37rem; }
.searchBox>a>img,.searchBox>a>span{ vertical-align:middle; }
.searchBox>a>span{ font-size:0.32rem; color:#717171; }
.keyText{ margin-top:0.15rem; }
.keyText>li{ display:inline-block; margin-right:0.34rem; font-size:0; }
.keyText>li>a{ font-size:0.26rem; color:white; }
/* ���� */
.profit{ margin-top:0.53rem; padding-bottom:0.53rem; }
.profit>div{ display:inline-block; vertical-align:middle; }
.yesterday>p{ margin-bottom:0.2rem; }
.yesterday>p>span{ font-size:0.4rem; color:white; }
.yesterday>p>img{ width:0.53rem; margin-left:0.2rem; }
.yesterday>input{ width:2.5rem; font-size:0.58rem; color:white; background:none; }
.earnProfit{ margin-left:1rem; }
.earnProfit>div{ display:inline-block; }
.shareProfit{ padding-right:0.37rem; border-right:0.01rem solid white; }
.storeProfit{ padding-left:0.37rem; }
.earnProfit>div>p{ margin-bottom:0.2rem;  }
.earnProfit>div>p>span{ font-size:0.34rem; color:white; }
.earnProfit>div>input{ width:1.9rem; font-size:0.34rem; color:white; background:none; }

/* ���� */
.nav{ margin:0.26rem 0; }
.nav>ul{ font-size:0; border-top:0.01rem solid #ddd; background:white; }
.nav>ul>li{ width:3.33rem; height:3.33rem; display:inline-block; text-align:center; position:relative; border-bottom:0.01rem solid #ddd; border-right:0.01rem solid #ddd; }
.nav>ul>li:nth-child(3),.nav>ul>li:nth-child(6),.nav>ul>li:nth-child(9),.nav>ul>li:nth-child(12){ border-right:none; }
.nav>ul>li>a{ width:100%; display:inline-block;  }
.nav>ul>li>a>i{ width:1rem; height:1rem; display:inline-block; margin-top:0.8rem; }
.nav>ul>li>a>i>img{ width:100%; height:100%; margin-bottom:0.2rem; }
.nav>ul>li>a>p{ font-size:0.4rem; color:#666666; }





