#list-breadcrumb li:nth-child(even) a:after {  border-left-color: #2980b9;}
#list-breadcrumb li:first-child a {  padding-left: 15px;  -webkit-border-radius: 4px 0 0 4px;  -moz-border-radius: 4px 0 0 4px;  -ms-border-radius: 4px 0 0 4px;  -o-border-radius: 4px 0 0 4px;  border-radius: 4px 0 0 4px;}
#list-breadcrumb li:first-child a:before {  border: none;}
#list-breadcrumb li:last-child a {  padding-right: 15px;  -webkit-border-radius: 0 4px 4px 0;  -moz-border-radius: 0 4px 4px 0;  -ms-border-radius: 0 4px 4px 0;  -o-border-radius: 0 4px 4px 0;  border-radius: 0 4px 4px 0;}
#list-breadcrumb li:last-child a:after {  border: none;}
#list-breadcrumb li a:before, #list-breadcrumb li a:after {  content: "";  position: absolute;  top: 0;  border: 0 solid #3498db;  border-width: 20px 10px;  width: 0;  height: 0;}
#list-breadcrumb li a:before {  left: -20px;  border-left-color: transparent;}
#list-breadcrumb li a:after {  left: 100%;  border-color: transparent;  border-left-color: #3498db;}
#list-breadcrumb li a:hover {  background-color: #1abc9c;}
#list-breadcrumb li a:hover:before {  border-color: #1abc9c;  border-left-color: transparent;}
#list-breadcrumb li a:hover:after {  border-left-color: #1abc9c;}
#list-breadcrumb li a:active {  background-color: #16a085;}
#list-breadcrumb li a:active:before {  border-color: #16a085;  border-left-color: transparent;}
#list-breadcrumb li a:active:after {  border-left-color: #16a085;}


