#breadcrumb {  list-style: none;  display: inline-block; left:0; top:50px;}
#breadcrumb img{width: 70%;   padding-left: 5px;  padding-top: 5px;}
#breadcrumb .icon {  font-size: 14px;}
#breadcrumb li { clear:both; margin-top:25px;}
#breadcrumb li a {  color: #FFF;  display: block;  background: #3498db;  text-decoration: none;  position: relative; width:30px;  /*height: 30px;  line-height: 30px; */ text-align: center;  margin-right: 23px; font-size:12px; padding-bottom:5px;}
#breadcrumb li:nth-child(even) a {  background-color: #2980b9;}
#breadcrumb li:nth-child(even) a:before {  border-color: #2980b9;  border-top-color: transparent;}
#breadcrumb li:nth-child(even) a:after {  border-top-color: #2980b9;}
#breadcrumb li:first-child a {   -webkit-border-radius: 4px 0 0 4px;  -moz-border-radius: 4px 0 0 4px;  -ms-border-radius: 4px 0 0 4px;  -o-border-radius: 4px 0 0 4px;  border-radius: 4px 0 0 4px;}
#breadcrumb li:first-child a:before {  border: none;}
#breadcrumb li:last-child a {   -webkit-border-radius: 0 4px 4px 0;  -moz-border-radius: 0 4px 4px 0;  -ms-border-radius: 0 4px 4px 0;  -o-border-radius: 0 4px 4px 0;  border-radius: 0 4px 4px 0;}
#breadcrumb li:last-child a:after {  border: none;}
#breadcrumb li a:before, #breadcrumb li a:after {  content: "";  position: absolute;  left: 0;  border: 0 solid #3498db;  border-width: 10px 15px;  width: 0;  height: 0;}
#breadcrumb li a:before {  top: -20px;  border-top-color: transparent;}
#breadcrumb li a:after {  top: 100%;  border-color: transparent;  border-top-color: #3498db;}
#breadcrumb li a:hover {  background-color: #1abc9c;}
#breadcrumb li a:hover:before {  border-color: #1abc9c;  border-top-color: transparent;}
#breadcrumb li a:hover:after {  border-top-color: #1abc9c;}
#breadcrumb li a:active {  background-color: #16a085;}
#breadcrumb li a:active:before {  border-color: #16a085;  border-top-color: transparent;}
#breadcrumb li a:active:after { border-top-color: #16a085;}


