/* 登录页面开始  */ 
body {
	background:#0b69e8;
	background-size: cover;
	font-family:Microsoft YaHei;
	background-attachment: fixed;
    background-position: center;
}
.container {
	width: 720px;
	height:430px;
	margin: 50px auto;
	
}
h1.header_1 {
    text-align: center;
    font-size: 40px;
    font-weight: normal;
    padding-top: 50px;
    color: #fff;
    font-family:Microsoft YaHei;
}

.formFields {
	width:400px;
	margin-left:33px;
	}

.contact_form {
	float:left;
	 }
.userLogin {
	float:left;
	}
/* === List Styles === */
.contact_form ul {
	list-style-type:none;list-style-position:outside; 
	padding:0px;
	}
.contact_form li { 
	width:260px;
	margin-top:20px;
	padding:3px;
	position:relative; 
	list-style:none;
	color:#FFFFFF;
	} 

/* === Form Elements === */
.contact_form input { 
    height:40px; 
	border:2px solid #fff;
	padding:5px 5px 5px 55px;
	outline:none;
	border-radius:25px;
	background-color:transparent;
	width:190px;
	color:#FFFFFF;

	
}
.userName_rec {
	position:absolute;
    top:190px;
	margin-left:3px;
	
	z-index:9999;
	
	padding:5px 10px;
	color:#fff;
	border:2px solid #43d854;
	border-top:none;
	
	margin-top:-10px;
	padding-top:20px;
	}

.list{
	width:250px;
	height:100px;
	font-size:14px;
	border:2px solid #43d854;
	background:url(../image/list_bg.png) no-repeat;
	border-top:none;
	margin-top:-30px;
	border-bottom-left-radius:25px;
	border-bottom-right-radius:25px;
	padding-left:30px;
	position:absolute;
	z-index:999;
}
.list ul li {
	padding-left:30px;
	height:20px;
	line-height:20px;
	
	}
.list ul li:nth-of-type(2){
	border-bottom-left-radius:22px;
	border-bottom-right-radius:22px;	
}
.yanzhengma {
	float:left;
	background:none;
	border:2px solid #fff;
	
	}
/* === HTML5 validation styles === */	
/*用户名*/
.contact_form input.userName { 
	/*background:#0b69e8 url('../image/red_asterisk.png') no-repeat 5px center;*/
	background:#0b69e8 url('../image/red_asterisk.png') no-repeat 5px center;
	border:2px solid #fff;
	}
/*点击用户名输入框*/
.contact_form input.userName:focus:invalid {
	background:#0b69e8 url('../image/invalid.png') no-repeat 5px center;
	border-color:#43d854;
	
}



/*输入密码*/
.contact_form input.userPassw:required {
	background:#0b69e8 url('../image/userPassw-red_asterisk.png') no-repeat 5px center;
}
/*点击密码输入框*/
.contact_form input.userPassw:focus:invalid {
	background:#0b69e8 url('../image/userPassw-invalid.png') no-repeat 5px center;
	border-color:#43d854;
}

/*输入邮箱*/
.contact_form input.userEmail:required {
	background:#0b69e8 url('../image/userEmail-red_asterisk.png') no-repeat 5px center;
	/*border-color: #fe9d50;*/
}
/*点击邮箱输入框*/
.contact_form input.userEmail:focus:invalid {
	background:#0b69e8 url('../image/userEmail-invalid.png') no-repeat 5px center;
	border-color:#43d854;
}

/*验证码*/
.contact_form input.yanzhengma:required {
	background:#0b69e8 url('../image/yanzhengma-red_asterisk.png') no-repeat 5px center;
}
/*点击验证码输入框*/
.contact_form input.yanzhengma:focus:invalid {
	background:#0b69e8 url('../image/yanzhengma-invalid.png') no-repeat 5px center;
	border-color:#43d854;
}
/*验证码输入不符合规则*/
.contact_form input.yanzhengma_err {
	background:#0b69e8 url('../image/yanzhengma-err.png') no-repeat 5px center;
	border-color: #fe9d50;
}
/*验证码输入正确*/
.contact_form input.yanzhengma:required:valid {
	background:#0b69e8 url('../image/yanzhengma-valid.png') no-repeat 5px center;
	border-color:#fff;
}

/* 右侧信息提示 */
.form_hint {
	background:#43d854;
	height:26px;
	line-height:26px;
	font-size:12px;
	border-radius: 3px 3px 3px 3px;
	color: white;
