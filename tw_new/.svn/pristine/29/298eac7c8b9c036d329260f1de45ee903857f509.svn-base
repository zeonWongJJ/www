#top_nav {
	margin: 0 auto;
	z-index: 1000;
	width:100%;
	height:70px;
}
#top_nav nav{
	display: table;
	height:70px;
	margin: 0 auto;
	position: relative;
	width:1200px;
}
#top_nav nav .logo{
	position: relative;
	float: left;
	margin: 15px 0;
	cursor: pointer;
	border-radius: 3px;
	background-image: url("../image/logo2.png");
	background-repeat: no-repeat;
	background-position: left;
	background-size: 80%;
	background-color: #3f8654;
	width:150px;
	height:40px;
}
#top_nav nav>.logo>i{
	position: absolute;
	right: 0;
	top:0;
	padding: 5px;
	font-size: 20px;
	align-self: center;
	color: #ffffff;
	transition: all 0.3s cubic-bezier(0.68, -0.55, 0.27, 1.55);
	transform:rotate(0deg);
	-ms-transform:rotate(0deg); 	/* IE 9 */
	-moz-transform:rotate(0deg); 	/* Firefox */
	-webkit-transform:rotate(0deg); /* Safari 和 Chrome */
	-o-transform:rotate(0deg);
}
.menu{
	z-index: 1000;
	display: none;
