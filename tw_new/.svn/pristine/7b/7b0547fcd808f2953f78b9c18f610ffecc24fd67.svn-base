.typeDetail>font>em,.typeDetail>font>span{ font-style:normal; }
.typeText>p{ width:7.5rem; height:1.32rem; color:#666; font-size:0.3rem; text-overflow:ellipsis; display: -webkit-box; overflow:hidden; isplay: -webkit-box; -webkit-box-orient; -webkit-line-clamp:3; -webkit-box-orient: vertical }
.typeDetail{ position:relative; font-size:0.32rem; }
.typeDetail>em{ display:inline-block; margin-right:0.42rem; font-size:0.32rem; font-style:normal; }
.typeDetail>span>em{ font-style:normal; }
