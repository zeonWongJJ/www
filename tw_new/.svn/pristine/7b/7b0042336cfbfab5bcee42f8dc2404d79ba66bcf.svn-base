/* ������� */
.productClassification{ width:1440px; margin:0 auto; font-size:0; }
.roomManagers{ margin-top:20px; margin-left:22px; }
.roomManagers>p{ height:53px; line-height:53px; padding-left:25px; font-size:16px; background:white; }

/* �����б� */
.room_List{ margin-top:20px; background:white; }
.room_List>form{ padding-top:20px; margin-left:24px; }
.room_List>form>a{ display:inline-block; vertical-align:middle; padding:8px 16px; font-size:14px; border:1px solid #e8e8e8; color:#a8a7a6; border-radius:14px; background:#f4f1ee; cursor:pointer; }
.room_List>form>a>img{ width:10px; vertical-align:middle; margin-right:10px; }
.searchRoom{ display:inline-block; }
.searchRoom>input{ width:134px; vertical-align:middle; margin-left:20px; padding:8px 10px; font-size:12px; border:1px solid #dfdfdf; background:#fafafa; border-bottom-left-radius:6px; border-top-left-radius:6px; }
.searchRoom>i{ width:30px; height:32px; vertical-align:middle; display:inline-block; border:1px solid #b7b6b3; background:#f4f0ee; border-bottom-right-radius:6px; border-top-right-radius:6px; }
.searchRoom>i>img{ width:20px; margin-top:6px; margin-left:6px; }

/* Ʒ�� */
.cateList{ margin-top:20px; padding:0 20px; }
.varieties{ padding:10px 0; border-bottom:1px solid #f4f4f4; }
.cateList>li>em,.varieties>em{ text-align:center; vertical-align:middle; }
.cateList img{ vertical-align:middle; }
.cateHead{ height:53px; line-height:53px; background:#f4f7fc; }
.cateHead>em,.varieties>em,.varietiesCateList>a>em,.varietiesCateList>em>div>em{ display:inline-block; vertical-align:middle; }
.cateHead>em{ font-size:14px; }
.varieties>em{ font-size:13px; }
.cateHead>em,.cateHead>em>p>span{ font-weight:bold; }
.cateHead>em>p,.varieties>em>p,.varietiesCateList>a>em>p,.varietiesCateList>em>div>em>p{ display:inline-block; }
.v1>img{ width:22px; margin-left:20px; }
.v1>p{ margin-left:15px; }
.v1>p>span{ vertical-align:middle; }
.v9>img{ width:34px; }
.v10>img{ width:14px; margin:0 5px; }
.v1{ width:160px; }
.v2{ width:120px; }
.v3{ width:260px; }
.v4{ width:200px; }
.v5{ width:118px; }
.v6{ width:112px; }
.v7{ width:100px; }
.v7>img{ width:36px; cursor:pointer; }
.v8{ width:94px; }
.v8>img{ width:14px; cursor:pointer; margin:0 5px; }
.license{ cursor:pointer; color:#95837f; }


/* �ײ�ѡ�� */
.bottomTool{ width:268px; height:50px; line-height:50px; display:inline-block; margin:20px 0; margin-left:20px;  background:white; }
.bottomTool>a{ display:inline-block; margin-left:26px; }
.bottomTool>a>span{ font-size:14px; cursor: pointer; }
.bottomTool>a>img{ vertical-align:middle; }
.bottomTool>a>span{ vertical-align:middle; margin-left:7px; }
.bottomAllSelect>img{ width:22px; }
.bottomDelect>img{ width:16px; }
.bottomProvisional>img{ width:16px; }

/* ��ҳ */
.page{ float:right; margin-top:30px; margin-right:20px; }
.page>a,.page>span{ display:inline-block; vertical-align:middle; padding:5px 10px; margin-left:10px; color:#999; font-size:14px; background:white; }
.page>a:hover{ background:#6e5c58; color:white; }

/* ��Ҫ��ʾ */
.pop_tips{ position:fixed; top:30%; left:50%; padding:20px; font-size:14px; background:white; box-shadow:3px 5px 23px #888888; }
.pop_tips>p{ display:inline-block; }
.pop_tips>img{ width:15px; }
.closeTips{ float:right; }
.tipsText>p{ margin-top:5px; }
.tipsText>p>s{ vertical-align:middle; text-decoration:none; }
.btnBox{ margin-top:10px; text-align:center; }
.btnBox>span,.btnBox>em{ display:inline-block; padding:3px 15px; }
.btnBox>span{ margin:0 15px; background:#FFCE1C; border:1px solid #EFAF17; color:#BA4611; cursor: pointer; }
.btnBox>em{ border:1px solid #ddd; cursor: pointer; }
