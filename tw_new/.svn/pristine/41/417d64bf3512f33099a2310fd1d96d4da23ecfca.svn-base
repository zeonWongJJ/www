.productClassification{ width:1440px; margin:0 auto; font-size:0; }

/* 修改积分盈余 */
.accountingAmount{ padding-bottom:15px; font-size:15px; background:white; }
.accountAm>p{ padding:12px 15px; background:#FBFBFB; border-bottom:1px solid #ddd; }
.accountAm>p>span{ color:#58AFE8; font-weight:bold; }
.accountAm>p>img{ float:right; }
.accountAm>form{ padding:20px; }
.accountAm>form>ul>li{ margin-top:15px; }
.accountAm>form>ul>li>span{ width:70px; text-align:right; display:inline-block; margin-right:10px; }
.accountAm>form>ul>li>span,.editAdmin>form>ul>li>em>span{ color:#A6A6A6; font-size:14px; }
.accountAm>form>ul>li>em>img,.editPoint>form>ul>li>em>span,.editPoint>form>ul>li>em>em{ vertical-align:middle; color:#999; font-size:14px; }
.accountAm>form>ul>li>input,.accountAm>form>ul>li>textarea{ padding:5px; border:1px solid #ddd; }
.accountAm>form>ul>li>textarea{ height:70px; vertical-align:top; }
.accountAm>form>ul>li>input,.accountAm>form>ul>li>em>img,.accountAm>form>ul>li>em>span{ vertical-align:middle; }
.totalAccount>p{ display:inline-block; font-size:14px; color:#A6A6A6; }
.totalAccount>p>em{ font-weight:bold; color:red; }
.correct>em{ font-size:14px; color:#A6A6A6; }
.editPrice>em span,.editPrice>em>em,.accountRemarks>em{ font-size:14px; color:#A6A6A6; }

#accountSub{ display:inline-block; padding:6px 15px; margin-left:106px; color:#a33300; background:#ffce1c; border:1px solid #e69c14; }






