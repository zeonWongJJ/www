/* 发布公告 */
.productClassification{ width:1440px; margin:0 auto; font-size:0; }
.announcement{ margin-top:20px; margin-left:22px; }
.announcement>p{ height:53px; line-height:53px; padding-left:25px; font-size:16px; background:white; }

.ggTitle{ margin-top:20px; }
.ggTitle{ background:#fbfbfb; border-bottom:1px solid #ddd; }
.ggTitle>span{ display:inline-block; padding:15px 22px; font-size:16px; border:1px solid #ddd; border-top:2px solid #ffb900; border-bottom:none; background:white; }
.announcementList{ margin-left:22px; padding-bottom:20px; background:white; }
.announcementList>form{ padding:20px; }
.announcementList>form>ul>li{ margin-top:15px; }
.announcementList>form>ul>li>span{ width:70px; text-align:right; display:inline-block; vertical-align:middle;  margin-right:10px; }
.announcementList>form>ul>li>span,.announcementList>form>ul>li>em>span{ color:#A6A6A6; font-size:14px; }
.announcementList>form>ul>li>input{ padding:5px; border:1px solid #ddd; }
.announcementList>form>ul>li>input,.announcementList>form>ul>li>em>img,.announcementList>form>ul>li>em>span{ vertical-align:middle; }

#announceSub{ display:inline-block; padding:6px 15px; margin-left:106px; margin-top:15px; font-size:14px; color:#a33300; background:#ffce1c; border:1px solid #e69c14; }



