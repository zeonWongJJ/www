}
.temperature .temType{
	margin-top: 0.466666rem;
}
.temperature .temType i{
	display: inline-block;
	width: 0.933333rem;
	height: 0.933333rem;
	border: 1px solid #bebebe;
	border-radius: 50%;
	box-sizing: border-box;
	font-style: normal;
	margin-right: 0.4rem;
	font-size: 0.24rem;
	color: #000000;
	text-align: center;
	line-height: 0.933333rem;
	margin-bottom: 0.133333rem;/*10*/
