}
.add{
	width: 100%;
	/*padding-bottom: 0.2rem;*/
	border-bottom: 1px solid #E4E4E4;
	margin-left: 0.133333rem;
}
.add .temType s{
	text-decoration: none;
}
.add .temType .s1{
	text-decoration: none;
	display: block;
	width: 0.16rem;
	height: 0.16rem;
	text-align: center;
	line-height: 0.16rem;	
	margin-left: 0.373333rem;/*28*/
	margin-top: 0.16rem;/*12*/
	margin-bottom: 0.093333rem;/*7*/
	display: none;
}
.add .temType .s2{
    display: block;
	height: 0.293333rem;
	text-align: center;
	line-height: 0.32rem;
	margin-top: 0.32rem;/*24*/
}
.cup .temType i.cupCur{/*选中的杯型*/
	background: #FF6633;
	color: white;
	border: none;
}
.temperature1 .temType i.temCur{/*选中的温度*/
	background: #FF6633;
	color: white;
	border: none;
}
.add .temType i.addCur{/*选中的果肉*/
	background: #FF6633;
	color: white;
	border: none;
}
/*加*/
.add .temType i.addCur .s1{
	display: block;
}
.add .temType i.addCur .s2{
	margin-top: 0;
}
