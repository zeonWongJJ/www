	color: white;
}
 .starName{
 	margin-top:0.4rem;
	margin-left: 0.4rem;
	/*position: absolute;
	left: 0;
	bottom: 0.8rem;
	bottom: 0.933333rem;/*改*/*/
}
 .star i{
	display: inline-block;
	width: 0.453333rem;
