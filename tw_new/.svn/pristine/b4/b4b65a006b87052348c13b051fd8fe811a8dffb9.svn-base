body{
	background: #f1f1f1;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
	border-bottom: 1px solid #d7d7d7;
}
.main .head i{
	font-style: normal;
}
.main .head a{
	margin-top: 0.066666rem;
	width: 0.32rem;
	position: absolute;
	left: 0.533333rem;
	top: 0.866666rem;
}
.main .tit{
	font-size: 0.4rem;
	color: #000000;
	line-height: 1.533333rem;
	padding-left: 0.533333rem;
}
.main .intDiv{
	padding-left: 1.44rem;
	margin-bottom: 0.933333rem;
}
.main .intDiv input{
	display: inline-block;
	width: 1.2rem;
	height: 1.2rem;
	background: white;
	border-right: 1px solid #efefef;
	float: left;
	text-align: center;
	line-height: 1.2rem;
	font-size: 0.4rem;
}
.main .submitBox a{
	display: block;
	width: 8.866666rem;
	height: 1.2rem;
	background: #e10030;
	border-radius: 0.266666rem;
	margin: 0 auto;
	font-size: 0.453333rem;
	color: white;
	text-align: center;
	line-height: 1.2rem;
	box-shadow:0 0.066666rem 0.066666rem #ffb9c8;
	margin-bottom: 0.133333rem;
}
.main .forget{
	font-size: 0.32rem;
	color: #6492e3;
	float: right;
	margin-right: 0.586666rem;
	margin-top: 0.133333rem;
}
/*新加的css*/
.typeBox .two{/*隐藏第二步*/
	display: none;
}
.typeBox .three{/*隐藏第三步*/
	display: none;
}




