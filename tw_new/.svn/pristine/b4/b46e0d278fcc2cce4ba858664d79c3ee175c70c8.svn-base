body{
	background: white;
}
html,body{
	overflow:inherit;
}
.main{
	width: 100%;
	height: 100%;
}
.main .backUp{
	width:0.8rem;
	display:inline-block;
	vertical-align:bottom;
	margin-left:0.3rem;
	margin-bottom:0.3rem;
}
.main .title{
	width: 1.8rem;
	height: 1.066666rem;
	display:inline-block;
	vertical-align:middle;
	background: #ff6633;
	margin-top: 1.226666rem;
	margin-left: 7%;
	position: relative;
}
.title p{
	font-size: 0.48rem;
	color: #000000;
	font-weight: 600;
	position: absolute;
	left: 0.466666rem;
	top: 0.2rem;
	width: 3.333333rem;
	letter-spacing: 0.04rem;
}
.main .nav{
	margin-top: 0.4rem;
	margin-left: 0.733333rem;
}
.main .nav ul{
	font-size: 0;
}
.main .nav li{
	display: inline-block;
	margin-right: 0.533333rem;
}
.nav li a{
	display: block;
	font-size: 0.346666rem;
	padding-bottom: 0.04rem;
}
.nav li.current a{
	color: #fd7100;
	border-bottom: 2px solid #ff6633;
}
.main .content{
	margin-top: 1.466666rem;
	margin-left: 7%;
	width: 93%;
	overflow-x: auto;
	padding-bottom: 1.066666rem;
}
.content ul{
	width: 200%;	
}
.content li{
	width: 8.533333rem;
	float: left;
	margin-right: 0.133333rem;
}
.content .pic{
	position: relative;
}
.content .pic .img{
	width: 8.533333rem;
}
.content .pic .img>img{
	height:6rem;
}
.content .pic .ding{
	position: absolute;
	left: 0.333333rem;
	bottom: 0.266666rem;
}
.pic .ding .name{
	font-size: 0.426666rem;
	color: #FFFFFF;
}
.pic .start{
	font-size: 0;
	margin-top: 0.08rem;
}
.pic .start i{
	display: inline-block;
	width: 0.466666rem;
	height: 0.466666rem;
	background: url(../images/xing_03.png) no-repeat;
	background-size: 100%;
	vertical-align: middle;
	margin-right: 0.133333rem;
}
.pic .start i.harfStart{
	background: url(../images/xing_05.png) no-repeat;
	background-size: 100%;
}
.pic .start span{
	display: inline-block;
	font-size: 0.48rem;
	color: white;
	vertical-align: middle;	
}
.content li .describe{
	font-size: 0.32rem;
	color: #333333;
	margin-top: 0.4rem;
	text-indent: 0.56rem;
	padding: 0 0.106666rem;
}
.pic .ding2{
	width: 0.493333rem;
	position: absolute;
	right: 0.533333rem;
	top: 0.4rem;
}

