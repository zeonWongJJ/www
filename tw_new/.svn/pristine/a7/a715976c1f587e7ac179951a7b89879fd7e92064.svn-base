.addTag>input,.addTag>a>img,.addTag>a>span{ vertical-align:middle; }
.addTag>a>span{ margin-left:5px; font-size:14px; color:#999; }
.moveTag{ cursor:pointer; }

.dragBox{ padding:0 15px; }
.dragBox>p{ margin-bottom:15px; }
.dragBox>p>span{ font-size:15px; font-weight:bold; }
.dragBox>p>em{ float:right; font-size:15px; color:#999; }
.dragBox>.item { position:relative; display: inline-block; cursor:pointer; padding:5px 15px; margin: 0 20px 20px 0; background-color: #F9F9F9;}
.dragBox>.item>span{ display: block; font-size:15px; text-align:center; color:#999;}
.dragBox>.item>img{ width:12px; position:absolute; right:-3px; top:-5px; }

.disabledDragBox{ padding:0 15px; }
.disabledDragBox>p{ margin-bottom:15px; }
.disabledDragBox>p>span{ font-size:15px; font-weight:bold; }
.disabledDragBox>p>em{ float:right; font-size:15px; color:#999; }
.disabledDragBox>.item { position:relative; display: inline-block; cursor:pointer; padding:5px 15px; margin: 0 20px 20px 0; background-color: #F9F9F9;}
.disabledDragBox>.item>span{ display: block; font-size:15px; text-align:center; color:#999;}
.disabledDragBox>.item>img{ width:12px; position:absolute; right:-3px; top:-5px; }
.tagSure{ padding-bottom:15px;  }
.tagSure>span{ display:inline-block; padding:6px 15px; margin-left:70px; font-size:14px; margin-top:15px; color:#a33300; background:#ffce1c; border:1px solid #e69c14; }

/* 动态预览 */
.aynaPreview{ width:780px; position:fixed; right:0; top:20%; padding:15px 30px; font-size:14px; z-index:2; background:white; }
.aynaPreview img{ vertical-align:middle; }
.aynaPreview>em>span{ font-size:12px; color:#ffce1c; vertical-align:middle; }
.previewTitle{ margin-top:25px; border-bottom:1px solid #DDD;  }
.previewTitle>h1{ margin-bottom:15px; font-size:20px; font-weight:bold; }
.viewOperation{ float:right; margin-top:14px; margin-right:24px; }
.viewOperation>i{ width:26px; height:26px; line-height:26px; text-align:center; display:inline-block; background:#f6f9fd; border:1px solid #ddd; cursor:pointer; }
.viewOperation>p{ text-align:right; margin-top:10px; color:#999; font-size:12px; }
/* 动态信息列表 */
.aynaPreview{ box-shadow:3px 5px 23px #888888; }
.aynaPreview>img{ float:right; cursor:pointer; }
.previewLeft,.previewRight{ vertical-align:top; }
.previewLeft{ width:448px; height:460px; overflow-y:scroll; display:inline-block; margin-top:25px;  }
.previewLeft::-webkit-scrollbar { width:8px; border-left:2px solid #ffffff; border-right: 2px solid #ffffff; background-color:#b8b8b8; }
.previewLeft::-webkit-scrollbar-thumb {width:8px; height:50px; background-color: #f3c727;}
.dynBox{ margin-top:25px; }
.dynBox:first-child{ margin-top:0; }
.headPortrait{ display:inline-block; }
.headPortrait,.previewList{ vertical-align:top;  }
