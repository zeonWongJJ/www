/* 锟斤拷锟斤拷锟斤拷 */
.cafeOrders{ position:absolute; top:20%; left:45%; padding:15px 20px; background:white; box-shadow:3px 5px 23px #888888; }
.cafeOrders>div{ margin-top:5px; }
.cafeOrders>div>em{ display:inline-block; vertical-align:middle; }
.cafeOrders>div>em>i{ width:8px; height:8px; display:inline-block; border-radius:50%; }
.cafeOrders>div>em>hr{ width:1px; height:60px; margin:0 auto; background:#000; border:0; }
.ordersContent{ width:305px; display:inline-block; vertical-align:top; margin-top:-2px; margin-left:10px; }
.ordersContent>h4{ font-size:12px; font-weight:bold; }
.ordersContent>p{ font-size:12px; font-weight:bold; color:#666666; }
.ordersContent>p>span{ width:200px; display:inline-block; font-weight:bold; color:#666666; }
.ordersContent>p>em{ display:inline-block; margin-right:30px; font-size:14px; color:#666666; }
.ordersContent>p>dfn{ font-style:normal; font-size:14px; }
.ordersNums>em>i{ border:4px solid #009944; }
.ordersTime>em>i{ border:4px solid #ff9500; }
.takeOver>em>i{ border:4px solid #dc1c5d; }
.placeOrders>em>i{ border:4px solid #23b6ae; }
.redPacket{ margin-top:10px; }
.redPacket>em,.distributionFee>em{ display:inline-block; padding:5px 10px; font-size:12px;  color:white; font-weight:bold; }
.redPacket>em{ background:#dc1c5d; }
.distributionFee{ margin-top:5px; }
.distributionFee>em{ background:#6e5c58; }
.redPacket>span,.distributionFee>span{ float:right; margin-top:6px; font-size:14px; font-weight:bold; color:#666666; }

.weChatPay{ margin-top:20px; }
.weChatPay>span{ font-size:12px; color:#666666; font-weight:bold; }
.weChatPay>em{ float:right; font-size:24px; font-weight:bold; color:#dc1c5d; }
.closeLay{ display:block; text-align:center; }
.closeLay>span{ display:inline-block; padding:10px 20px; font-size:12px; background:#43cb83; color:white; border-radius:5px; cursor:pointer; }

/* 鍒嗛〉 */
.page{ float:right; margin-top:30px; margin-right:20px; }
.page>a,.page>span{ display:inline-block; vertical-align:middle; padding:5px 10px; margin-left:10px; color:#999; font-size:14px; background:white; }
.page>a:hover{ background:#6e5c58; color:white; }