.dynamicList>p>span{ font-size:16px; }
.tagShell{ float:right; padding:8px 15px; margin-top:-8px; margin-right:20px; border:1px solid #ddd; border-radius:22px; background:#eae7e5; cursor:pointer; }
.tagShell>span,.tagShell>img{ vertical-align:middle; }
.tagShell>span{ margin-left:5px; font-size:14px; color:#999; }

/* 动态分类 */
.dynamicCate{ margin-top:15px; background:white; }
.dynamicCate ul{ padding:0 30px; }
.dynamicCate ul>li{ padding:20px 10px; border-bottom:1px solid #ddd; }
.dynamicCate ul>li:last-child{ border:none; }
