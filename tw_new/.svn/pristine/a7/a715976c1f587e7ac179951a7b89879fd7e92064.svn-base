.dynamicTag>a{ display:inline-block; float:right; margin-top:-5px; padding:5px 15px; cursor:pointer; border:1px solid #ddd; border-radius:22px; }
.dynamicTag>a>span,.dynamicTag>a>img{ vertical-align:middle; }
.dynamicTag>a>span{ margin-left:10px; font-size:15px; color:#999; }
.auditStatusBox,.displayStatusBox,.releaseTimeBox{ width:100px; position:relative; border:1px solid #fff; padding:5px 10px; margin:0 10px; }
.auditStatus,.displayStatus,.releaseTime{ width:100px; padding:5px 10px; position:absolute; left:-1px; border:1px solid #ddd; border-top:none; background:white; }
.auditStatus>a,.displayStatus>a,.releaseTime>a{ height:30px; line-height:30px; display:block; text-align:left; padding-left:22px; font-size:14px; cursor:pointer; }
.auditStatus>a:hover,.displayStatus>a:hover,.releaseTime>a:hover{ color:#6e5c58; }


.dynamicTag>a{ display:inline-block; float:right; margin-top:-5px; padding:5px 15px; cursor:pointer; border:1px solid #ddd; border-radius:22px; }
.dynamicTag>a>span,.dynamicTag>a>img{ vertical-align:middle; }
.dynamicTag>a>span{ margin-left:10px; font-size:15px; color:#999; }
.auditStatusBox,.displayStatusBox,.releaseTimeBox{ width:100px; position:relative; border:1px solid #fff; padding:5px 10px; margin:0 10px; }
.auditStatus,.displayStatus,.releaseTime{ width:100px; padding:5px 10px; position:absolute; left:-1px; border:1px solid #ddd; border-top:none; background:white; }
.auditStatus>a,.displayStatus>a,.releaseTime>a{ height:30px; line-height:30px; display:block; text-align:left; padding-left:22px; font-size:14px; cursor:pointer; }
.auditStatus>a:hover,.displayStatus>a:hover,.releaseTime>a:hover{ color:#6e5c58; }

/* 选择动态 */
.dynamic_content{ margin-top:15px; }
.dynamicBox>li{ margin-top:15px; padding:10px 20px; background:white; }
.choiceDynamic>em{ font-size:12px; text-align:center; }
.choiceDynamic>em,.choiceDynamic img,.dynamicInfo>p{ vertical-align:middle; }
.dynamicInfo{ margin-left:5px; }
.choiceDynamic>em,.dynamicInfo,.dynamicInfo>p{ display:inline-block; }
.dynamicInfo>p{ margin-left:5px; }
.dynamicInfo>img{ width:42px; border-radius:10px; }
.dynamicInfo>p>em{ display:block; font-size:13px; }
.dynamicInfo>p>span{ font-size:12px; }
.v1{ width:200px; }
.v2{ width:325px; cursor:pointer; }
.v2:hover{ color:#d19354; }
.v3{ width:230px; }
.v4{ width:200px; }
.v5{ width:100px; }
.v6{ width:80px; }
.v1>img{ width:22px; }
.v3>span{ display:inline-block; padding:5px 10px; background:#f4f8f9; }
.v4 a>span,.v4 img{ cursor:pointer; vertical-align:middle; }
.v5>img,.v6>img{ margin:0 5px; cursor:pointer; }

/* 底部选择 */
.bottomTool{ height:50px; line-height:50px; display:inline-block; margin:10px 0; padding-right: 20px; background:white; }
.bottomTool>a{ display:inline-block; margin-left:26px; cursor: pointer}
.bottomTool>a>span{ font-size:14px; }
.bottomTool>a>img{ vertical-align:middle; }
.bottomTool>a>span{ vertical-align:middle; margin-left:7px; }
.bottomAllSelect>img{ width:23px; }
.bottomDelect>img{ width:16px; }
.bottomProvisional>img{ width:16px; }

/* 标签管理 */
.tagLay{ width:500px; position:fixed; top:40%; left:45%; background:white; box-shadow:3px 5px 23px #888888; }
