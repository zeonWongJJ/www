body{
	background:#f1f1f1;
}
html,body{
	overflow:inherit;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
}
.main .head i{
	font-style: normal;
}
.main .head a.back{
	margin-top: 0.066666rem;
	width: 0.853333rem;
	height: 0.666666rem;
	position: absolute;
	text-align: center;
	line-height: 0.666666rem;
	left: 0.266666rem;
	top: 0.8rem;
}
.main .head a.back img{
	width: 0.853333rem;
	height: 0.32rem;
}
.main .listBox{
	
}
.listBox li{
	/*margin: 0 0.24rem;*/
	margin: 0.4rem 0.24rem 0 0.24rem;
}
.listBox li a{
	display: block;	
}
.listBox li .time{
	font-size: 0.293333rem;
	color: #666666;
	text-align: center;
}
.listBox li .describe{
	background: white;
	border: 1px solid #e3e3e3;
	padding: 0 0.24rem;
	margin-top: 0.2rem;
}
.describe .title{
	font-size: 0.373333rem;
	color: #000000;
	margin-top: 0.333333rem;
}
.describe .miao{
	margin-top: 0.266666rem;
	margin-bottom: 0.333333rem;
}
.describe .miao .pic{
	display: inline-block;
	vertical-align: top;
	width: 1.666666rem;
	height: 1.666666rem;
	border-radius: 0.133333rem;
	border-radius: 0.133333rem;
	overflow: hidden;
	float: left;
}
.describe .miao .wenzi{
	float: left;
	display: inline-block;
	vertical-align: top;
	font-size: 0.346666rem;
	color: #666666;
	width: 6.4rem;
	margin-left: 0.2rem;
	display: -webkit-box !important; 
	overflow: hidden; 
	word-break: break-all; 
	text-overflow: ellipsis; 
	-webkit-box-orient: vertical; 
	-webkit-line-clamp: 4;
	line-height: 0.4rem;
}
.describe .miao .jiantou{
	float: right;
	margin-right: 0.133333rem;
	margin-top: 0.533333rem;
	width: 0.213333rem;
}
.listBox li.noPic .miao .wenzi{
	width: 8.333333rem;
	margin-left: 0;
	display: -webkit-box !important; 
	overflow: hidden; 
	word-break: break-all; 
	text-overflow: ellipsis; 
	-webkit-box-orient: vertical; 
	-webkit-line-clamp: 2;
}
.listBox li.noPic .miao .jiantou{
	margin-top: 0.2rem;
}


