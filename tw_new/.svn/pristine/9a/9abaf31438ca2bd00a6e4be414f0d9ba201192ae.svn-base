.editUser{ font-size:15px; background:white;  }
.editUser>p{ padding:12px 15px; color:#58AFE8; font-weight:bold; background:#FBFBFB; border-bottom:1px solid #ddd; }
.editUser>p>span{  }
.editUser>p>img{ float:right; }
.editUser>form{ padding:20px; }
.editUser>form>ul>li{ margin-top:15px; }
.editUser>form>ul>li>span{ width:70px; text-align:right; display:inline-block; margin-right:10px; }
.editUser>form>ul>li>span,.editUser>form>ul>li>em>span{ color:#A6A6A6; font-size:14px; }
.editUser>form>ul>li>input{ padding:5px; border:1px solid #ddd; }
.editUser>form>ul>li>input,.editUser>form>ul>li>em>img,.editUser>form>ul>li>em>span{ vertical-align:middle; }
#editUser_name,#editUser_name,#editUser_phone{ width:140px; }
#editUser_mail{ width:200px; }
#editUser_pwd,#editUser_rePwd{ width:180px; }
.editUser>em{ vertical-align:middle; color:#A6A6A6; font-size:14px; }
.editUser>em>img{ vertical-align:middle; }
.roleLevel{ display:inline-block; }
.roleLevel>a{ display:inline-block; position:relative; padding:8px 12px; margin-right:6px; font-size:14px; background:#f2f2f2; border-radius:5px;  }
.roleLevel>a>img{ width:18px; position:absolute; right:-3px; top:-8px; }
#userSub{ display:inline-block; padding:6px 15px; margin-left:87px; margin-top:15px; color:#a33300; background:#ffce1c; border:1px solid #e69c14; }
