/*  ��ҳ */
.index{ width:1440px; margin:0 auto; font-size:0; }


/* ��Ϣ�б� */
.infoList{ margin-top:20px; margin-left:20px; font-size:0; }
.infoList>a{ display:inline-block; margin-left:33px; }
.infoList>a:first-child{ margin:0; }
.infoList>a>.listL{ width:104px; height:90px; line-height:90px; display:inline-block; text-align:center; vertical-align:middle; }
.infoList>a>.listL>img{ width:38px; vertical-align:middle; }
.infoList>a>.listR{ width:168px;  display:inline-block; vertical-align:middle; padding:17px 0; background:white; }
.infoList>a>.listR>span{ display:inline-block; margin-bottom:8px; margin-left:10px; font-size:22px; font-weight:bold; color:black; }
.infoList>a>.listR>dfn{ display:inline-block; margin-left:10px; font-size:16px; color:#aaaaaa; }

.newOrders>.listL{ background:#ff7977; border:none;  }
.newUser>.listL{ background:#ffd75c; }
.newStore>.listL{ background:#5adc9c; }
.newShopkeeper>.listL{ background:#72beff; }

/*  ��Ӫ�ſ� */
.operate{ margin-top:20px; margin-left:20px; }
.operate>h3{ height:52px; line-height:52px;  padding-left:22px; font-size:16px; background:#fbfbfb; border-bottom:1px solid #e8e8e8; }
.operateContent{ background:white; }
.operateLeft{ width:288px; display:inline-block; vertical-align:top; margin-top:35px; margin-left:25px; padding-bottom:35px; }
.operateLeft>p{ height:91px; line-height:91px; padding-left:30px; font-size:16px; color:white; }
.operateLeft>p:first-child{ border-bottom:1px solid #8ec6eb; }
.operateLeft>p:last-child{ border-top:1px solid #a8d3f0; }
.operateLeft>p>span{ float:right;  margin-right:38px; font-size:16px; font-weight:bold; color:white; }
.volume{ background:#5faee3; }
.dailyOrders{ background:#6eb5e5; }
.totalVolume{ background:#7cbde8; }
.totalOrders{ background:#99cbed; }

/*  ��״ͼ */
.operateRight{ float:right; display:inline-block; margin-top:38px; margin-right:30px; }
/* ��ͼ */
.advertisement{ width:820px; display:inline-block; vertical-align:top; margin-top:35px; margin-left:25px; }
.advertisement>img{ width:820px; }

/* ���¶�̬ */
.newDynamic{ width:576px; display:inline-block; margin-top:20px; margin-left:20px;  }
.newDynamic>h3{ height:52px; line-height:52px; padding-left:20px; font-size:16px; background:#fbfbfb; border-bottom:1px solid #e8e8e8; }
.dynamicContent>li{ padding:12px 0; padding-left:25px; cursor: pointer; }
.dynamicContent>li:nth-child(odd){ background:white; }
.dynamicContent>li:nth-child(even){ background:#f6f7f9; }
.dynamicContent>li>img{ width:42px; vertical-align:middle; margin-right:12px; }
.dynamicInfo{ display:inline-block; vertical-align:middle; }
.dynamicInfo>span{ font-size:14px; }
.dynamicInfo>p{ font-size:12px; color:#b6b6b6; }
.viewMore{ height:40px; line-height:40px; text-align:right; padding:0 15px; background:white; }
.viewMore>a{ display:block; font-size:14px; color:#b6b6b6; vertical-align:middle; border-top:2px solid #d2d2d2; }
.viewMore>a>img{ width:20px; vertical-align:middle; margin-left:14px; }

/*  �������ٷֱ� */
.percentage{ width:614px; display:inline-block; vertical-align:top; margin-top:20px; margin-left:20px;  }
.percentage>h3{ height:52px; line-height:52px; padding-left:20px; font-size:16px; background:#fbfbfb; border-bottom:1px solid #e8e8e8; }
.monthBox{ background:white; }
.monthBox>a{ width:300px; display:inline-block; text-align:center; padding:30px 0; font-size:12px; }
.monthData{ display:inline-block; vertical-align:middle; margin-left:30px; }
.monthData>span{ font-weight:bold; }
.monthData>p{ font-size:20px; font-weight:bold; }
.monthData>p{ color:#ff7977; }
.beforeData>dl>dd{ width:49%; display:inline-block; }
.beforeData>dl>dd:last-child{ border-left:1px solid #ddd; }
.beforeData>dl{ margin-top:15px; }

/* 显示层 */
.showLay{ width:500px; height:500px; overflow-y:scroll; position:fixed; top:10%; left:26%; padding:15px 30px; background:white;  box-shadow:3px 5px 23px #888888; z-index:2; }
.showLay>h3{ text-align:center; padding:10px 0; font-size:15px; font-weight:bold; }
.toolBox{ text-align:center; }
.toolBox>img{ width:14px; vertical-align:middle; margin:0 5px; }
.toolBox>em{ margin-left:10px; font-size:12px; border-left:1px solid #ddd; }
.toolBox>em,.toolBox>em>span,.toolBox>em>s{ vertical-align:middle; text-decoration:none; color:#999; }
.toolBox>em>span{ margin-left:10px; }
.layTextBox>p{ margin-top:10px; text-indent:24px; font-size:12px; font-weight:bold; }
.layTextBox>i{ display:block; text-align:center; margin-top:10px; }
.displayBl{ cursor:pointer; }
.showLay img { max-width: 300px; cursor:pointer; }

/* 重要提示  */
.tips{ width:340px; position:fixed; top:50%; left:50%; padding:34px 34px 0 34px; font-size:14px; background:white; box-shadow:3px 5px 23px #888888; z-index:3; }
.tips>img{ position:absolute; right:20px; top:15px; }
.tips>p>span{ display:block; margin-top:5px; }
.tipsBtn{ text-align:center; margin-top:22px; }
.tipsBtn>em{ display:inline-block; padding:4px 10px; color:#aa4301; border:1px solid #e69c14; background:#ffce1c; cursor:pointer; }
.tipsBtn>a{ display:inline-block; padding:4px 10px; margin-left:28px; margin-bottom:20px; border:1px solid #ddd; cursor:pointer; }


.lay{ position:absolute; width:100%; top:0; background:white; opacity:0.5; z-index:1; }

.processingbar {
    display:inline-block ;
    vertical-align:middle;
    position: relative;
    margin:-7px 0 0 -5px;
}
.processingbar font {
    color: #ec1749;
    display: block;
    width: 100px;
    height: 100px;
    line-height: 100px;
    font-size:18px;
    text-align: center;
    position: absolute;
    left: 0;
    top: 0;
    margin: 0px 0 0 0px;
}
#processingbar2 font{
    color: #ffd861;
}
#processingbar3 font{
    color: #5adc9c;
}
#processingbar4 font{
    color: #72beff;
}


