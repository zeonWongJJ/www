/* �ҵ��㼣 */
.pjoTitle{ position:relative; }
.pjoTitle>a.back{ position:absolute; left:0.24rem; top:1.3rem; }
.pjoTitle>a.back>img{ width:0.85rem; }
.pjoTitle>a.edit,.pjoTitle>a.complete{ position:absolute; right:0.45rem; top:1.15rem; color:black; font-size:0.42rem; font-weight:bold; cursor:pointer; }
/* �б� */
.footprintList>dl>dt{ height:0.74rem; line-height:0.74rem; padding:0 0.3rem; }
.footprintList>dl>dt>a{ display:inline-block; }
.footprintList>dl>dt>a>img{ width:0.46rem; }
.footprintList>dl>dt>a>span{ display:inline-block; font-size:0.32rem; }
.footprintList>dl>dt>a>img,.footprintList>dl>dt>a>span{ vertical-align:middle; }
.footprintList>dl>dd{ padding-left:0.3rem; background:white; }
.footprintList>dl>dd>img,.footprintList>dl>dd>a,.footprintList>dl>dd>a>img,.footprintList>dl>dd>a>em{ vertical-align:middle; }
.footprintList>dl>dd>img{ width:0.46rem; }
.footprintList>dl>dd>a{ display:inline-block; padding:0.26rem 0 0.26rem 0.3rem; border-bottom:0.02rem solid #ddd; }
.footprintList>dl>dd:last-child>a{ border-bottom:none; }
.footprintList>dl>dd>a,.footprintList>dl>dd>a>em{ display:inline-block; }
.footprintList>dl>dd>a>img{ width:1.46rem; height:1.46rem; margin-right:0.1rem; border-radius:0.1rem; }
/*.footprintList>dl>dd>a>em{ width:7.16rem; }*/
.footprintList>dl>dd>a>em{ width:7.73rem; }
.footprintList>dl>dd>a>em>dfn>em{ font-size:0.4rem; font-weight:bold; font-style:normal; }
/*.footprintList>dl>dd>a>em>dfn>span{ margin-left:2.5rem; font-size:0.36rem; font-weight:bold; font-style:normal; color:#fe563c; }*/
.footprintList>dl>dd>a>em>dfn>span{ margin-left:3rem; font-size:0.36rem; font-weight:bold; font-style:normal; color:#fe563c; }
.footprintList>dl>dd>a>em>span{ width:6.6rem; height:0.44rem; white-space:nowrap; text-overflow:ellipsis; overflow:hidden; display:inline-block; font-size:0.28rem; font-style:normal; color:#666; }
.footprintList>dl>dd>a>em>p>span{ display:inline-block; font-size:0.32rem; font-style:normal; }
.footprintList>dl>dd>a>em>p>span:last-child{ margin-left:0.2rem; }
/* �ײ� */
.bottom{ width:100%; position:fixed; display:none; bottom:0; font-size:0; }
.bottom>a{ width:50%; height:1rem; line-height:1rem; display:inline-block; text-align:center; font-size:0.4rem; }
.cancel{ background:white; }
.delete{ background:#fe563c; color:white; }
/* ��ʾ�� */
.tips{ position:absolute; top:50%; left:4.2rem; padding:0.26rem; font-size:0.37rem; background:#303030; color:white; border-radius:0.2rem; display:none; }


