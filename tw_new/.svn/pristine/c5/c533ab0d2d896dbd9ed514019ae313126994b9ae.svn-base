/*支付弹框*/
.payment{ width:100%; position:fixed; bottom:0; z-index:2; background:white; display: none; }
.surplusTime{ height:1.22rem; line-height:1.22rem; position:relative; text-align:center; border-bottom:0.01rem solid #ddd; }
.surplusTime>span{ font-size:0.37rem; }
.surplusTime>span>em{ font-style:normal; color:#ff6633; }
.closeSurplus{ width:0.32rem; position:absolute; top:0.45rem; right:0.48rem; }
.payment>dl>dd{ height:1.22rem; line-height:1.22rem; position:relative; padding-left:0.53rem; border-bottom:0.01rem solid #ddd; }
.payment>dl>dd:last-child{ border-bottom:none; }
.payment>dl>dd>img,.payment>dl>dd>span{ vertical-align:middle; }
.payment>dl>dd>img{ width:0.82rem; margin-right:0.34rem; }
.payment>dl>dd>span{ font-size:0.34rem; }
.surePay{ text-align:center; background:#ff6633; }
.surePay>a>span{ font-size:0.42rem; color:white; }
.surePay>a>span>em{ font-style:normal; }
.surePay>a>span>em>i{ font-style: normal;}
.checkPay{ position:absolute; top:0.1rem; right:0.89rem; }
.checkPay>img{ width:0.46rem; }