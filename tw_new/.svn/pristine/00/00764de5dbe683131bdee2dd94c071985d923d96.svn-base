body{
	background: #f1f1f1;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
	border-bottom: 1px solid #d7d7d7;
}
.main .head i{
	font-style: normal;
}
.main .head a.back{
	margin-top: 0.066666rem;
	width: 0.32rem;
	position: absolute;
	left: 0.533333rem;
	top: 0.2rem;
}
.main .content{
	background: white;
	padding-left: 0.4rem;
	margin-top: 0.266666rem;
}
.content .validTime{
	padding: 0.266666rem 0.4rem 0.266666rem 0;	
	border-bottom: 1px solid #D7D7D7;
}
.content .validTime .h3{
	font-size: 0.346666rem;
	color: #333333;
	font-weight: bold;
	margin-bottom: 0.093333rem;
}
.content .validTime .wen{
	font-size: 0.346666rem;
	color: #333333;
	line-height: 0.613333rem;
}



