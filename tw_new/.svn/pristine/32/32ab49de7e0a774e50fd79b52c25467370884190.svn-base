body{
	background: #f1f1f1;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
	border-bottom: 1px solid #d7d7d7;
}
.main .head i{
	font-style: normal;
}
.main .head a.back{
	margin-top: 0.066666rem;
	width: 0.853333rem;
	height: 0.666666rem;
	position: absolute;
	text-align: center;
	line-height: 0.666666rem;
	left: 0.266666rem;
	top: 0.8rem;
}
.main .head a.back img{
	width: 0.853333rem;
	height: 0.32rem;
}
.main .head a.submit{
	position: absolute;
	right: 0.6rem;
	top: 0.906666rem;
	font-size: 0.32rem;
	color: #ff6633;
}
.main .shopName{
	margin-top: 0.266666rem;
	padding: 0.133333rem 0.4rem;
	font-size: 0;
	background: white;
}
.shopName .pic{
	display: inline-block;
	width: 0.8rem;
	height: 0.8rem;
	border-radius: 0.16rem;
	overflow: hidden;
	vertical-align: middle;
}
.shopName .name{
	display: inline-block;
	font-size: 0.293333rem;
	color: #000000;
	vertical-align: middle;
	margin-left: 0.2rem;
}
/*办公室评价*/
.main .offAppraise{
	margin-top: 0.266666rem;
	background: white;	
	padding-left: 0.4rem;
}
.offAppraise .title{
	font-size: 0.266666rem;
	color: #000000;	
	padding: 0.333333rem 0 0.2rem 0;
	border-bottom: 1px solid #F1F1F1;
}
.offAppraise .title i{
	font-style: normal;
	font-size: 0.213333rem;
	color: #999999;
	margin-left: 0.053333rem;
}
.appBox .roomType{
	padding-top: 0.2rem;
}
.appBox .roomType .rName{
	font-size: 0.293333rem;
	color: #000000;
	width: 60%;
	float: left;
	margin-top: 0.04rem;
}
.appBox .roomType .rFace{
	width: 30%;
	float: right;
	text-align: right;
	margin-right: 0.133333rem;
}
.appBox .roomType .rFace a{
	display: inline-block;
	width: 0.666666rem;
	height: 0.666666rem;
	margin-right: 0.2rem;
}
.roomType a.good{
	background: url(../images/yongping_11.png) no-repeat center center;
	background-size: 100%;
}
.roomType a.soso{
	background: url(../images/yongping_21.png) no-repeat center center;
	background-size: 100%;
}
.roomType a.bad{
	background: url(../images/yongping_15.png) no-repeat center center;
	background-size: 100%;
}
.roomType a.yGood{/*选中的好*/
	background: url(../images/yongping_20.png) no-repeat center center;
	background-size: 100%;
}
.roomType a.ySoso{/*选中的一般*/
	background: url(../images/yongping_13.png) no-repeat center center;
	background-size: 100%;
}
.roomType a.yBad{/*选中的不好*/
	background: url(../images/yongping_28.png) no-repeat center center;
	background-size: 100%;
}
.appBox .tag{
	font-size: 0;
}
.appBox .tag a{
	display: inline-block;
	padding: 0.093333rem 0.226666rem;
	border: 1px solid #f5f5f5;
	font-size: 0.226666rem;
	color: #999999;
	border-radius: 0.053333rem;
	margin-right: 0.173333rem;
	margin-top: 0.093333rem;
}
.appBox .tag a.tChoose{/*选中的标签*/
	color: #FF6633;
	background: #fff2ec;
	border:1px solid #ff6633;
}
.appBox .txtDiv{
	margin-top: 0.16rem;
	position: relative;
}
.txtDiv .txt{
	width: 96%;
	height: 1.866666rem;
	border: 1px solid #f5f5f5;
	border-radius: 0.133333rem;
	padding: 0.133333rem;
	box-sizing: border-box;
	line-height: normal;
	font-size: 0.226666rem;
	color: #000000;
}
.txtDiv .num{
	font-size: 0.226666rem;
	color: #999999;
	position: absolute;
	right: 0.533333rem;
	bottom: 0.133333rem;
}
.appBox .upload{
	margin-top: 0.2rem;
	padding-bottom: 0.266666rem;
}
