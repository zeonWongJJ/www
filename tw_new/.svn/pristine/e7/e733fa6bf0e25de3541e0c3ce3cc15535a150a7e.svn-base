/*分页*/
.page{ float:right; margin-top:30px; margin-right:20px; }
.page>a,.page>span{ display:inline-block; vertical-align:middle; padding:5px 10px; margin-left:10px; color:#999; font-size:14px; background:white; }
.page>a:hover{ background:#6e5c58; color:white; }

/*修改预警值弹框*/
.seatName{width: 440px; height: 240px; background:white; position: fixed; left: 50%; margin-left:-220px; top: 50%; margin-top: -120px; box-shadow: 0 0 20px #dedede; display: none;}
.seatName .title{height: 50px; border-bottom: 1px solid #e8e8e8; padding: 0 15px 0 30px;}
.seatName .title .h3{ color: #4ba6ea; font-size: 14px; display: inline-block; line-height: 50px;}
.seatName .title .close{display: block; width: 17px; height: 17px; background: url(../images/pro_19.png); float: right; margin-top: 17px;}
.seatName .inputBox{padding: 25px 0 0 40px; height: 50px;}
.seatName .inputBox .oldNum{ font-size: 12px; color: #666666; float: left; line-height: 30px; margin-left: 8px;}
.seatName .inputBox .biao{ float: left; font-size: 12px; color: #9c9fa5; line-height: 30px; width: 75px; text-align: right;}
.seatName .inputBox .int{ float: left; font-size:12px; width: 140px; height: 30px; border: 1px solid #d0cfce; border-radius: 2px; box-shadow: 5px 0 10px #efefef inset; padding: 0 10px; margin-left: 8px;}
.seatName .inputBox .red{ float:left; font-size: 12px; color: #676767; margin-left: 10px; line-height: 30px; display: none;}
.seatName .inputBox .red i{display: inline-block; width: 18px; height: 18px; background: url(../images/f_03.png); vertical-align: middle; margin-right: 8px;}
.seatName .sureBox a{ background:green; width: 85px; height: 28px; font-size: 14px; display: inline-block; background: #ffce1c; color: #a33300; text-align: center; line-height: 28px; border: 1px solid #e69c14; margin-left: 123px; margin-top: 25px;}
