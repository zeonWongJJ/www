body{
	background:#f1f1f1;
}
html,body{
	overflow:inherit;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
}
.main .head i{
	font-style: normal;
}
.main .head a.back{
	margin-top: 0.066666rem;
	width: 0.853333rem;
	height: 0.666666rem;
	position: absolute;
	text-align: center;
	line-height: 0.666666rem;
	left: 0.266666rem;
	top: 0.8rem;
}
.main .head a.back img{
	width: 0.853333rem;
	height: 0.32rem;
}
/*标签*/
.main .tagBox{
	background: white;
	margin-top: 0.266666rem;
	padding: 0.133333rem 0 0.266666rem 0.4rem;
}
.tagBox li{
	display: inline-block;
	/*float: left;*/
	margin-top: 0.16rem;
	margin-right: 0.16rem;
}
.tagBox li a{
	display: inline-block;
	font-size: 0.293333rem;
	color: #666666;
	padding: 0.133333rem 0.173333rem;
	border: 1px solid #c9c9c9;
	border-radius: 0.133333rem;
	box-sizing: border-box;
}
.tagBox li a i{
	font-style: normal;
}
.tagBox li.allClick a{/*点击全部的样式*/
	background: #FFF2EC;
	color: #FF6633;
	border: 1px solid #ff6633;
}
.tagBox li.otherClick a{/*点击其他的样式*/
	background: #ffb299;
	color: white;
	border: none;
}
/*评价*/
.main .appList{
	margin-top: 0.266666rem;
	background: white;
}
.appList li{
	font-size: 0;
	padding: 0 0.333333rem;
	margin-top: 0.466666rem;
}
.appList li .picL{
	display: inline-block;
	width: 0.8rem;
	height: 0.8rem;
	border-radius: 50%;
	overflow: hidden;
	vertical-align: top;
}
.appList li .describeR{
	display: inline-block;
	margin-left: 0.2rem;
	vertical-align: top;
	width: 8.266666rem;
	border-bottom: 1px solid #f5f5f5;
}
.describeR .name{	
	font-size: 0.32rem;
	color: #333333;
	position: relative;
}
.describeR .name .ming{
	display: inline-block;
	width: 6.4rem;
}
.describeR .name .shijian{
	position: absolute;
	right: 0;
	color: #999999;
}
.describeR .product{
	font-size: 0.32rem;
	color: #999999;
	margin-top: 0.066666rem;
}
.describeR .discuss{
	font-size: 0.32rem;
	color: #666666;
	margin-top: 0.066666rem;
}
.describeR .discuss .red{
	color: #FE563C;
	font-style: normal;
}
.describeR .imgBox{
	margin-top: 0.066666rem;
	margin-bottom: 0.133333rem;
}
.describeR .imgBox img{
	width: 2.4rem;
	height: 2.4rem;
	margin-right:0.133333rem ;
	margin-bottom: 0.133333rem;
}



