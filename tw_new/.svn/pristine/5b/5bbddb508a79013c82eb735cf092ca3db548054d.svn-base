body{
	background: #f1f1f1;
}
.main{
	width: 100%;
	height: 100%;
	margin:0 auto;
}
.main .head{
	height: 1.733333rem;
	text-align: center;
	font-size: 0.426666rem;
	padding-top: 0.853333rem;
	background: white;
	position: relative;
	border-bottom: 1px solid #d7d7d7;
}
.main .head i{
	font-style: normal;
}
.main .head a{
	margin-top: 0.066666rem;
	width: 0.32rem;
	position: absolute;
	left: 0.533333rem;
	top: 0.866666rem;
}
.main .fillBox{
	margin: 0.266666rem 0 0 0;
}
.main .fillBox li{
	padding-left: 0.2rem;
	background: white;
}
.main li div{
	width: 100%;
	height: 1.066666rem;
	background: white;
	font-size: 0.373333rem;
	color: #333333;
	line-height: 1.066666rem;
	border-bottom: 1px solid #d7d7d7;
	box-sizing: border-box;
}
.fillBox li span{
	display: inline-block;
	width: 23%;
}
.fillBox li input{
	display: inline-block;
	width: 75%;
	height: 1rem;
	background: white;
	font-size: 0.373333rem;
	color: #333333;
	line-height: 1rem;
	box-sizing: border-box;
	line-height:normal;
}
.fillBox li:last-child div{
	border-bottom: none;
}
.fillBox li{
	position: relative;
}
.fillBox li i{
	display: block;
	width: 1.066666rem;
	height: 1rem;	
	position: absolute;
	right: 0.026666rem;
	top: 0.053333rem;
	text-align: center;
	line-height: 1rem;
}
.fillBox li i.openEye{
	display: none;
}
.fillBox li i.closeEye{
	/*display: none;*/
}
.fillBox li i.openEye img{
	width: 0.506666rem;
	height: 0.253333rem;
}
.fillBox li i.closeEye img{
	width: 0.506666rem;
	height: 0.186666rem;
}
.main .forget{
	margin: 0.293333rem 0 0.506666rem 0;
}
.main .forget a{
	font-size: 0.373333rem;
	color: #666666;
	float: right;
	margin-right: 0.266666rem;
}

.main .submitBox a{
	display: block;
	width: 8.866666rem;
	height: 1.2rem;
	background: #e10030;
	border-radius: 0.266666rem;
	margin: 0 auto;
	font-size: 0.453333rem;
	color: white;
	text-align: center;
	line-height: 1.2rem;
	box-shadow:0 0.066666rem 0.066666rem #ffb9c8;
	margin-bottom: 0.133333rem;
}





