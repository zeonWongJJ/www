.tableBox .row:last-child{ border-bottom: none;}
.tableBox span{display: inline-block; vertical-align: top;}
.tableBox span:nth-child(1){ width: 125px; text-align: center;}
.tableBox span:nth-child(2){ width: 175px; padding-left: 10px;}
.tableBox span:nth-child(3){ width: 355px;}
