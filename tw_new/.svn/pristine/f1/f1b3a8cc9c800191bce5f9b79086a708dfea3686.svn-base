/***地址项***/
.address_item,.address_item1{
    position: relative;
    width:100%;
    padding: 20px 30px;
    box-sizing: border-box;
    background-image:url(../image/address_border.png);
    background-position: center ;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    border: 1px solid #cccccc;
}
.address_item h4{
    font-size: 14px;
}
.address_item p{
    height: 32px;
    overflow: hidden;
    margin: 10px 0;
}
.address_item label{
    opacity:0;
    position: absolute;
    width:0;
    height:0;
    right:0 ;
    bottom: 0;
    border-left:18px solid transparent;
    border-top:15px solid transparent;
    border-right:18px solid #3f8654 ;
    border-bottom:15px solid #3f8654 ;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=0);
}
.address_item label:after{
    content: "";
    position: absolute;
    width: 12px;
    height: 6px;
    background: transparent;
    top: -2px;
    left: 0;
    border: 3px solid #ffffff;
    border-top: none;
    border-right: none;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.add_address{
    display: table;
    padding: 46px 70px;
}
.add_address p{
    float: left;
    font-size: 14px;
    line-height:35px;
    margin: 0;
    color: #dcdcdc;
}
.add_address:hover p{
    color: #3f8654;
}
.add_address:hover i{
    color: #3f8654;
}
.add_address i{
    float: left;
    font-size: 30px;
    color: #dcdcdc;
    margin-right: 10px;
}
.consignee ul li a:hover .address_item,.consignee ul li a:hover .address_item1{
    background-image: url(../image/address_hover_border.png);
}
.consignee ul li a:hover .edit{
    opacity:1;
    transition: all 1s;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=100);
}

