/*地址弹框*/
.add_address_box{
    top:50%;
    left: 50%;
    margin-left: -350px;
    margin-top: -235px;
    position: relative;
    height:430px;
    padding: 20px;
    box-shadow: 0px 4px 16px rgba(0, 0, 0, 0.25);
    width:660px;
    background: #ffffff;
}
.add_address_box h2{
    font-size: 14px;
    color: #333333;
}
.add_address_box li h2{
    float: right;
    line-height:30px;
}
.add_address_box ul{
    display: table;
    padding-top: 20px;
}
.add_address_box div{
    text-align: right;
    float: left;
    width:80px;
}
.add_address_box li{
    display: table;
    padding: 10px 0;
}
.add_address_box input,
.add_address_box select{
    line-height:30px;
    float: left;
    margin: 0 10px;
    width:140px;
    box-sizing: border-box;
    height:30px;
    border: 1px solid #bfbfbf;
    padding:0 10px ;
    color: #333333;
    box-shadow: inset 3px 3px 10px rgba(0, 0, 0, 0.07)
}
.add_address_box  button{
    color: #333333;
    width:130px;
    height:30px;
    box-sizing: border-box;
    border: 1px solid #cccccc;
    margin:10px 0 50px 90px;
    background: #f5f5f5;
}
.add_address_box i {
    margin-top: 7px;
    float: left;
    display: block;
    position: relative;
    width:16px;
    height:16px;
    border-radius: 50%;
    margin-right: 10px;
}
.add_address_box span{
    line-height:30px;
    color: #ee2222;
    font-size: 14px;
}
.add_address_box p{
    float: left;
}
.add_address_box p span{
    color: #333333;
}
.add_address_box a{
    position: absolute;
    right: 0;
    top:0;
    width:44px;
    height:44px;
}
.add_address_box em{
    display: block;
    margin: 20px 0 0 12px;
    width:20px;
    height:2px;
    background: #333333;
    webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.add_address_box em:after{
    left: 9px;
    top:-9px;
    content: "";
    position: absolute;
    height:20px;
    width:2px;
    background: #333333;

}
.add_address_box .empty{
    background: #ee2222;
}
.add_address_box .empty:after{
    position: absolute;
    content: "";
    background: #ffffff;
    width:8px;
    height:2px;
    top:7px;
    left: 4px;
}
.add_address_box .pass{
    background: #3b7e4f;
}
.add_address_box .pass:after{
    content: "";
    position: absolute;
    width: 8px;
    height: 4px;
    background: transparent;
    top: 3px;
    left: 4px;
    border: 2px solid #ffffff;
    border-top: none;
    border-right: none;
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    -o-transform: rotate(-45deg);
    -ms-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

